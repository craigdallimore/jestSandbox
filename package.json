{
  "name": "jestSandbox",
  "version": "0.0.0",
  "description": "Jest sandbox",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "collectCoverage": false,
    "bail": true,
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ],
    "testDirectoryName": "__tests__",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Hubrus",
  "devDependencies": {
    "babel-jest": "^9.0.3",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "jest-cli": "^0.9.2"
  },
  "dependencies": {
    "ramda": "^0.19.1"
  }
}
